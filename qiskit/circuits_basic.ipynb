   "source": [
    "from qiskit import QuantumCircuit, QuantumRegister"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "de7a4ca5",
   "metadata": {},
   "outputs": [],
   "source": [
    "qubits = QuantumRegister(2, name=\"q\")\n",
    "circuit = QuantumCircuit(qubits)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "601c6cfd",
   "metadata": {},
   "outputs": [],
   "source": [
    "q0, q1 = qubits\n",
    "circuit.h(q0)\n",
    "circuit.cx(q0, q1)\n",
    "circuit.measure_all()"
   ]
  },
